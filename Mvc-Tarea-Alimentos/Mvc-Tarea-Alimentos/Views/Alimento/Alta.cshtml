
@{
    ViewBag.Title = "Alta";
}

<h2>Alta</h2>

<form action="/Alimento/DarAlta" method="post">
    <div class="form-group">
    <input class="form-control"type="text" name="nombre" placeholder="Nombre del alimento"/>
    </div>
    <div class="form-group">
    <input class="form-control"type="text" name="peso" placeholder="Peso del alimento"/>
    </div>
    <div class="form-group">
    <input class="btn btn-success"type="submit" value="Enviar" />
    </div>
</form>